<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/history"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/White" >

        <Button
            android:id="@+id/normal"
            android:layout_width="fill_parent"
            android:layout_height="80dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/yellowbuttons"
            android:text="@string/normal"
            android:textAppearance="@style/ButtonTextMedium" />

        <Button
            android:id="@+id/under"
            android:layout_width="fill_parent"
            android:layout_height="80dp"
            android:layout_above="@+id/normal"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/greenbuttons"
            android:text="@string/under"
            android:textAppearance="@style/ButtonTextMedium" />

        <Button
            android:id="@+id/over"
            android:layout_width="fill_parent"
            android:layout_height="80dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/normal"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/redbuttons"
            android:text="@string/over"
            android:textAppearance="@style/ButtonTextMedium" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/under"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:text="CHOOSE PRODUCTION LEVEL"
            android:textAppearance="@style/TextMedium" />

        <Button
            android:id="@+id/historyc"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/over"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="5dp"
            android:text="@string/history" />

    </RelativeLayout>
