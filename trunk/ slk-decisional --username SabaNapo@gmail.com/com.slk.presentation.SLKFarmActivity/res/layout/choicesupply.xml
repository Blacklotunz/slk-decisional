<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/history"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/White" >

        <Button
            android:id="@+id/normal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="@drawable/yellowbuttons"
            android:text="@string/normal"
            android:textAppearance="@style/ButtonTextMedium" />

        <Button
            android:id="@+id/under"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/normal"
            android:background="@drawable/greenbuttons"
            android:text="@string/under"
            android:layout_marginBottom="10dp"
            android:textAppearance="@style/ButtonTextMedium" />


        <Button
            android:id="@+id/over"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/normal"
            android:layout_marginTop="10dp"
            android:background="@drawable/redbuttons"
            android:text="@string/over"
            android:textAppearance="@style/ButtonTextMedium" />

        <Button
            android:id="@+id/historyc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:text="@string/history" />

        <Button
            android:id="@+id/back"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="@string/back" />

    </RelativeLayout>
